<!-- Exercise 1.1: CDN Setup -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Bootstrap CDN Setup</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
  <h1 class="text-center mt-5">Bootstrap via CDN</h1>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>


<!-- Exercise 1.2: Local Bootstrap Files -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Local Bootstrap</title>
  <link href="css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
  <h1 class="text-center mt-5">Bootstrap via Downloaded Files</h1>
  <script src="js/bootstrap.bundle.min.js"></script>
</body>
</html>
